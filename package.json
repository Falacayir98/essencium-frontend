{
  "name": "@frachtwerk/essencium-frontend",
  "version": "0.1.0",
  "private": true,
  "description": "This is a React-based starter monorepo consisting of a component library, docs and a boilerplate.",
  "keywords": [
    "starter",
    "boilerplate",
    "frontend",
    "react",
    "mantine"
  ],
  "homepage": "https://github.com/Frachtwerk/essencium-frontend/blob/master/README.md",
  "bugs": {
    "url": "https://github.com/Frachtwerk/essencium-frontend/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:Frachtwerk/essencium-frontend.git"
  },
  "license": "MIT",
  "author": "Frachtwerk GmbH",
  "scripts": {
    "build": "NX_BRANCH=$(git rev-parse --abbrev-ref HEAD) nx build app",
    "clean": "nx run-many --target=clean && rimraf dist node_modules",
    "clean:install": "pnpm i -w && pnpm clean && pnpm install",
    "dev": "nx dev app",
    "lint": "NX_BRANCH=$(git rev-parse --abbrev-ref HEAD) nx affected --target=lint --parallel=3",
    "format": "NX_BRANCH=$(git rev-parse --abbrev-ref HEAD) nx affected --target=format --parallel=3",
    "setup-env": "pnpm install && husky install",
    "test": "NX_BRANCH=$(git rev-parse --abbrev-ref HEAD) nx affected --target=test --parallel=3",
  },
  "devDependencies": {
    "@commitlint/cli": "17.4.2",
    "@commitlint/config-conventional": "17.4.2",
    "@nrwl/eslint-plugin-nx": "15.5.3",
    "@typescript-eslint/parser": "5.48.2",
    "eslint": "8.32.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-simple-import-sort": "9.0.0",
    "husky": "8.0.3",
    "nx": "15.5.3",
    "prettier": "2.8.3",
    "rimraf": "4.1.1",
    "typescript": "4.9.4"
  },
  "engines": {
    "node": ">=16",
    "pnpm": "7"
  },
  "volta": {
    "node": "16.19.0"
  },
  "dependencies": {
    "@nrwl/nx-cloud": "15.0.2"
  }
}
